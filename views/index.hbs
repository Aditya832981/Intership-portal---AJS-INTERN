  {{!-- services section --}}
  <section id="services_sectio" class="py-4">
    <div class="container">
      <h1 class="text-center text-secondary">Explore from the best</h1>

      <div class="row mt-5">
        {{!-- first service column --}}
        {{#each services}}

        <div class="col-md-4">
          <div class="card">
            <div class="cardbody">
              <div class="container text-center py-3">
                <i class="{{this.icon}} service_icon"></i>
              </div>
              <h5 class="card-title">{{this.title}}</h5>
              <p>{{this.description}}</p>

              <a href="{{this.link}}">{{this.linkText}}</a>
              <a href="https://www.google.com/" class="ml-3">Support</a>
            </div>
          </div>

        </div>
        {{/each}}



      </div>
    </div>

  </section>



